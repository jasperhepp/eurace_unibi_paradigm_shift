pes>\n\n   <dataType>\n    <name>consumption_request</name>\n    <description>Used by  mall to hold demand</description>\n    <variables>\n     <variable>\n      <type>int</type>\n      <name>worker_id</name>\n      <description></description>\n     </variable>\n     <variable>\n      <type>int</type>\n      <name>consumer_region_id</name>\n      <description></description>\n     </variable>\n     <variable>\n      <type>int</type>\n      <name>firm_id</name>\n      <description></description>\n     </variable>\n     <variable>\n      <type>double</type>\n      <name>quantity</name>\n      <description></description>\n     </variable>\n    </variables>\n   </dataType>\n   <dataType>\n    <name>mall_info</name>\n    <description>Used by firm to hold information about sales in malls</description>\n    <variables>\n     <variable>\n      <type>int</type>\n      <name>mall_id</name>\n      <description></description>\n     </variable>\n     <variable>\n      <type>double</type>\n      <name>critical_stock</name>\n      <description></description>\n     </variable>\n     <variable>\n      <type>double</type>\n      <name>current_stock</name>\n      <description></description>\n     </variable>\n    </variables>\n   </dataType>\n   <dataType>\n    <name>mall_quality_price_info</name>\n    <description>Used by household to hold information about quality and prices of one good offered by mall</description>\n    <variables>\n     <variable>\n      <type>int</type>\n      <name>mall_id</name>\n      <description></description>\n     </variable>\n     <variable>\n      <type>int</type>\n      <name>firm_id</name>\n      <description></description>\n     </variable>\n     <variable>\n      <type>int</type>\n      <name>mall_region_id</name>\n      <description></description>\n     </variable>\n     <variable>\n      <type>double</type>\n      <name>quality</name>\n      <description></description>\n     </variable>\n     <variable>\n      <type>double</type>\n      <name>price<