ich the agent is associated.</description>\n    </variable>\n    <variable>\n     <type>int</type>\n     <name>gov_id</name>\n     <description>Government  to which the agent is associated.</description>\n    </variable>\n    <variable>\n     <type>int</type>\n     <name>partition_id</name>\n     <description>Partition ID used for parallelization.</description>\n    </variable>\n   </memory>\n   <functions/>\n  </xagent>\n  <xagent>\n   <name>Government</name>\n   <description></description>\n   <memory>\n    <variable>\n     <type>int</type>\n     <name>id</name>\n     <description>Agent ID.</description>\n    </variable>\n    <variable>\n     <type>int_array</type>\n     <name>list_of_regions</name>\n     <description>List of regions that are associated to the government.</description>\n    </variable>\n    <variable>\n     <type>double</type>\n     <name>payment_account</name>\n     <description>Current account value.</description>\n    </variable>\n    <variable>\n     <type>int</type>\n     <name>day_of_month_to_act</name>\n     <description>Activation day.</description>\n    </variable>\n    <variable>\n     <type>int</type>\n     <name>region_id</name>\n     <description>Region ID to which the agent is associated.</description>\n    </variable>\n    <variable>\n     <type>int</type>\n     <name>partition_id</name>\n     <description>Partition ID used for parallelization.</description>\n    </variable>\n   </memory>\n   <functions/>\n  </xagent>\n  <xagent>\n   <name>CentralBank</name>\n   <description></description>\n   <memory>\n    <variable>\n     <type>int</type>\n     <name>id</name>\n     <description>Agent ID.</description>\n    </variable>\n    <variable>\n     <type>int</type>\n     <name>region_id</name>\n     <description>Region ID to which the agent is associated.</description>\n    </variable>\n    <variable>\n     <type>int</type>\n     <name>partition_id</name>\n     <description>Partition ID used for parallelization.</description>\n    </variable>\n   </memory>\n   <functions/>\n  </xagent>\n  <xagent>\n   <name>ClearingHouse</name>\n   <description></description>\n   <memory>\n    <variable>\n     <type>int</type>\n     <name>id</name>\n     <description>Agent ID.</description>\n    </variable>\n    <variable>\n     <type>int</type>\n     <name>region_id</name>\n     <description>Region ID to which the agent is associated.</description>\n    </variable>\n    <variable>\n     <type>int</type>\n     <name>partition_id</name>\n     <description>Partition ID used for parallelization.</description>\n    </variable>\n   </memory>\n   <functions/>\n  </xagent>\n </agents>\n <messages>\n  <message>\n   <name>bank_account_update</name>\n   <description>Sent by agents to inform their bank about the current payment account balance.</description>\n   <variables>\n    <variable>\n     <type>int</type>\n     <name>id</name>\n     <description>Sending agent ID.</description>\n    </variable>\n    <variable>\n     <type>int</type>\n     <name>bank_id</name>\n     <description>Receiving bank agent ID.</description>\n    </variable>\n    <variable>\n     <type>double</type>\n     <name>payment_account</name>\n     <description>Current account value.</description>\n    </variable>\n   </variables>\n  </message>\n  <message>\n   <name>bank_to_central_bank_account_update</name>\n   <description>Banks send account information to central bank</description>\n   <variables>\n    <variable>\n     <type>int</type>\n     <name>id</name>\n     <description>Sending agent ID.</description>\n    </variable>\n    <variable>\n     <type>double</type>\n     <name>payment_account</name>\n     <description>Current account value.</description>\n    </variable>\n    <variable>\n     <type>double</type>\n     <name>ecb_debt</name>\n     <description>Debt of the bank to the Central Bank</description>\n    </variable>\n   </variables>\n  </message>\n  <message>\n   <name>gov_to_central_bank_account_update</name>\n   <description>Governments sen