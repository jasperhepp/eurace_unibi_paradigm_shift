  </variable>\n    <variable>\n     <type>int</type>\n     <name>gov_id</name>\n     <description>Government  to which the agent is associated.</description>\n    </variable>\n    <variable>\n     <type>int</type>\n     <name>day_of_month_to_act</name>\n     <description>Activation day.</description>\n    </variable>\n    <variable>\n     <type>double</type>\n     <name>payment_account</name>\n     <description>Current account value.</description>\n    </variable>\n    <variable>\n     <type>int</type>\n     <name>bank_id</name>\n     <description>Bank ID at which the agent maintains its payment account.</description>\n    </variable>\n    <variable>\n     <type>int</type>\n     <name>partition_id</name>\n     <description>Partition ID used for parallelization.</description>\n    </variable>\n   </memory>\n   <functions/>\n  </xagent>\n  <xagent>\n   <name>Mall</name>\n   <description></description>\n   <memory>\n    <variable>\n     <type>int</type>\n     <name>id</name>\n     <description>Agent ID.</description>\n    </variable>\n    <variable>\n     <type>int</type>\n     <name>region_id</name>\n     <description>Region ID to which the agent is associated.</description>\n    </variable>\n    <variable>\n     <type>int</type>\n     <name>gov_id</name>\n     <description>Government  to which the agent is associated.</description>\n    </variable>\n    <variable>\n     <type>int</type>\n     <name>partition_id</name>\n     <description>Partition ID used for parallelization.</description>\n    </variable>\n   </memory>\n   <functions/>\n  </xagent>\n  <xagent>\n   <name>IGFirm</name>\n   <description></description>\n   <memory>\n    <variable>\n     <type>int</type>\n     <name>id</name>\n     <description>Agent ID.</description>\n    </variable>\n    <variable>\n     <type>int</type>\n     <name>region_id</name>\n     <description>Region ID to which the agent is associated.</description>\n    </variable>\n    <variable>\n     <type>int</type>\n     <name>gov_id</nam